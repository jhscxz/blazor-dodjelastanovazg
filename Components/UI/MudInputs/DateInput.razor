@using MudBlazor
@using System.Globalization

<MudDatePicker ShrinkLabel
               @bind-Date="InternalValue"
               Label="@Label"
               Editable="true"
               DateFormat="@DateFormat"
               Culture="@Culture"
               Variant="Variant.Outlined"
               Margin="Margin.Dense"
               Class="mb-2"
               Required="@Required" />

